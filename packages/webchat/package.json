{
    "name": "@cognigy/webchat",
    "version": "1.0.0",
    "description": "> TODO: description",
    "author": "Robin Schuster <r.schuster@cognigy.com>",
    "license": "ISC",
    "main": "lib/webchat/index.js",
    "scripts": {
        "build": "tsc",
        "bundle": "yarn webchat && yarn webchat-legacy && yarn messenger && yarn messenger-legacy && yarn speech-input && yarn speech-input-legacy && yarn speech-output && yarn speech-output-legacy && yarn persistent-menu && yarn persistent-menu-legacy",
        "dev": "webpack-dev-server --config webpack.dev.js --content-base dist/ --host 0.0.0.0 --disable-host-check",
        "webchat": "webpack --config webpack.production.js",
        "webchat-legacy": "webpack --config webpack.production.legacy.js --output dist/webchat.legacy.js",
        "messenger": "webpack --config webpack.production.js --entry ./src/plugins/message/messenger/index.tsx --output dist/messenger.webchat-plugin.js",
        "messenger-legacy": "webpack --config webpack.production.legacy.js --entry ./src/plugins/message/messenger/index.tsx --output dist/messenger.webchat-plugin.legacy.js",
        "speech-input": "webpack --config webpack.production.js --entry ./src/plugins/input/speech/index.tsx --output dist/speech-input.webchat-plugin.js",
        "speech-input-legacy": "webpack --config webpack.production.legacy.js --entry ./src/plugins/input/speech/index.tsx --output dist/speech-input.webchat-plugin.legacy.js",
        "speech-output": "webpack --config webpack.production.js --entry ./src/plugins/message/speech-output/index.tsx --output dist/speech-output.webchat-plugin.js",
        "speech-output-legacy": "webpack --config webpack.production.legacy.js --entry ./src/plugins/message/speech-output/index.tsx --output dist/speech-output.webchat-plugin.legacy.js",
        "persistent-menu": "webpack --config webpack.production.js --entry ./src/plugins/input/persistent-menu/index.tsx --output dist/persistent-menu.webchat-plugin.js",
        "persistent-menu-legacy": "webpack --config webpack.production.legacy.js --entry ./src/plugins/input/persistent-menu/index.tsx --output dist/persistent-menu.webchat-plugin.legacy.js"
    },
    "dependencies": {
        "@cognigy/webchat-client": "1.0.0",
        "@emotion/cache": "^10.0.0",
        "@emotion/core": "^10.0.6",
        "@emotion/provider": "^0.11.2",
        "@emotion/styled": "^10.0.6",
        "emotion": "^10.0.6",
        "emotion-theming": "^10.0.6",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-emotion": "^10.0.0",
        "react-player": "^1.9.3",
        "react-redux": "^6.0.0",
        "react-responsive-carousel": "^3.1.47",
        "redux": "^4.0.1",
        "styled-jss": "^2.2.3",
        "tinycolor2": "^1.4.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-object-assign": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@babel/runtime": "^7.2.0",
        "@types/react": "^16.7.18",
        "@types/react-dom": "^16.0.11",
        "@types/react-redux": "^6.0.12",
        "@types/tinycolor2": "^1.4.1",
        "babel-loader": "^8.0.4",
        "babel-polyfill": "^6.26.0",
        "css-loader": "^2.1.0",
        "react-svg-loader": "^2.1.0",
        "style-loader": "^0.23.1",
        "svg-inline-loader": "^0.8.0",
        "svg-react-loader": "^0.4.6",
        "ts-loader": "^5.3.3",
        "typesafe-actions": "^3.0.0",
        "typescript": "^3.2.2",
        "webpack": "^4.28.4",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14",
        "whatwg-fetch": "^3.0.0"
    }
}
